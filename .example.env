# Default environment for maximum-quality profile

# LLM backend (Ollama)
LLM_BACKEND=ollama
IS_OLLAMA_MODEL=true
OLLAMA_BASE_URL=https://ollama.example.ru
OLLAMA_MODEL=gpt-oss:latest
OLLAMA_MODE=chat
OLLAMA_FORCE_JSON=true

# Timeouts and retries
LLM_HTTP_TIMEOUT=300
OLLAMA_MAX_RETRIES=3
OLLAMA_BACKOFF_SECONDS=2
OLLAMA_NUM_PREDICT=1024

# Prompt/context limits
LLM_SCHEMA_MAX_CHARS=3000
LLM_PROMPT_MAX_CHARS=1000
SQLGLOT_ENABLE=true

# Search/quality knobs
PIPE_MAX_ATTEMPTS=3
PIPE_STRATEGY_MAX=3
PIPE_PROBE_TOP=3
PIPE_STRATEGY_GATE_SCORE=0

# Trino (override per deployment)
# TRINO_JDBC_URL=jdbc:trino://host:443?user=...&password=...&catalog=data&schema=public
# TRINO_CATALOG=data
# TRINO_SCHEMA=public

